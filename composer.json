  "keywords": ["sitemap"],
  "homepage": "https://github.com/jlaso/sitemap-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Joseluis Laso",
          "homepage": "http://www.joseluislaso.es"
      }
  ],
  "minimum-stability": "dev",
  "require": {
      "php": ">=5.3.2",
      "symfony/framework-bundle": ">=2.1,<2.3-dev",
      "symfony/security-bundle": ">=2.1,<2.3-dev"
  },
  "autoload": {
      "psr-0": { "Jlaso\\SitemapBundle": "" }
